{
  "name": "something-something-ui",
  "description": "a basic component library",
  "bugs": {
    "url": "https://github.com/something-something-ui/issues"
  },
  "homepage": "https://github.com/rowanhen//something-something-ui#readme",
  "main": "./dist/index.js",
  "author": "rowan henseleit",
  "keywords": [
    "react",
    "components",
    "component library"
  ],
  "files": [
    "dist"
  ],
  "version": "1.0.4",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build": "tsc -p tsconfig.build.json",
    "check-types": "tsc --noEmit",
    "build-storybook": "storybook build",
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "release": "standard-version",
    "pre-commit-lint": "npm run check-types && lint-staged"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addons": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/react-vite": "^7.6.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "standard-version": "^9.5.0",
    "storybook": "^7.6.16",
    "storybook-dark-mode": "^3.0.3",
    "styled-components": "^6.1.6",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rowanhen/something-something-ui.git"
  }
}
